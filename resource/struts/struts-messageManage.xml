<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN" "http://www.opensymphony.com/xwork/xwork-1.0.dtd">
<xwork>
    
    <package name="messageManage" namespace="/message" extends="struts-default">
		<action name="messageAction" class="messageAction" method="messageSend" >
		<result name="success">/page/main.jsp</result>
		<result name="fail"></result>
		</action>
		<action name="searchMessageAction" class="searchMessageAction" method="getSearchMessage">
		<result name="success">/page/showMessage.jsp</result>
		<result name="fail"></result>
		</action>
	</package>
</xwork>